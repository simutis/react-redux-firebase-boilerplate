{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "max-len": ["warn", 120],
    "indent": ["warn", 2, {"SwitchCase": 1}],
    "react/jsx-indent": ["warn", 2],
    "no-console": 0,
    "comma-dangle": 0,
    "react/prop-types": 0,
    "arrow-parens": 0
  },
  "globals": {
    "localStorage": true 
  }
}